{
  "name": "@ampath/esm-morgue-app",
  "version": "5.4.1",
  "description": "Morgue app",
  "keywords": [
    "openmrs"
  ],
  "homepage": "https://github.com/ampath/ampath-esm-morgue-app#readme",
  "bugs": {
    "url": "https://github.com/ampath/ampath-esm-morgue-app/issues"
